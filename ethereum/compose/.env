GETH_NET=rinkeby

GETH_CONFIG_DIR=/etc/geth
HOST_GETH_DATA_DIR=/operations/ethereum/geth/data
GETH_DATA_DIR=/root/.ethereum

CONFIG-Eth-SyncMode=light
CONFIG-Node-DataDir=/root/.ethereum
CONFIG-Node-HTTPHost=0.0.0.0
CONFIG-Node-HTTPCors=['*']
CONFIG-Node-HTTPVirtualHosts=['*']
CONFIG-Metrics-Enabled=true
CONFIG-Metrics-HTTP=0.0.0.0
